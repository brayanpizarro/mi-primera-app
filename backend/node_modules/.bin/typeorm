#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/braya/Desktop/paguina web/mi-primera-app/backend/node_modules/.pnpm/typeorm@0.3.23_pg@8.16.0_re_f46a4dca1651a371562ddc33d70a51ad/node_modules/typeorm/node_modules:/mnt/c/Users/braya/Desktop/paguina web/mi-primera-app/backend/node_modules/.pnpm/typeorm@0.3.23_pg@8.16.0_re_f46a4dca1651a371562ddc33d70a51ad/node_modules:/mnt/c/Users/braya/Desktop/paguina web/mi-primera-app/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/braya/Desktop/paguina web/mi-primera-app/backend/node_modules/.pnpm/typeorm@0.3.23_pg@8.16.0_re_f46a4dca1651a371562ddc33d70a51ad/node_modules/typeorm/node_modules:/mnt/c/Users/braya/Desktop/paguina web/mi-primera-app/backend/node_modules/.pnpm/typeorm@0.3.23_pg@8.16.0_re_f46a4dca1651a371562ddc33d70a51ad/node_modules:/mnt/c/Users/braya/Desktop/paguina web/mi-primera-app/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
